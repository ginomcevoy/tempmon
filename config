# File containing hostnames to monitor
HOST_FILE=hosts

# Frequency of monitoring in seconds
SENSOR_INTERVAL=3600

# Directory that will hold output data
OUTPUT_DIR=data

# Other parameters
LOG_FILE=monitor.log
PID_FILE=monitor.pid
BMC_USER=admin
BMC_PASS=admin
DATE_FORMAT='%Y-%m-%d %H:%M:%S'
